{
  "name": "anaphygonask-nodejs",
  "version": "2.0.0",
  "description": "Enhanced AI Q&A website powered by Gemini API",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:maximized": "MAXIMIZED_MODE=true nodemon app.js",
    "test:api": "curl -X POST http://localhost:3006/api/chat -H 'Content-Type: application/json' -d '{\"question\":\"Test maximized API\"}'",
    "health": "curl http://localhost:3006/health",
    "metrics": "curl http://localhost:3006/api/metrics",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "ai",
    "chatbot",
    "gemini",
    "nodejs",
    "express",
    "artificial-intelligence",
    "question-answering"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3"
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  },
  "bugs": {
    "url": "your-issues-url"
  },
  "homepage": "your-homepage-url"
}
